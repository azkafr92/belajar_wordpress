{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"settings": {
		"appearanceTools": true,
		"typography": {
			"fontFamilies": [
				{
					"name": "Montserrat",
					"slug": "montserrat",
					"fontFamily": "\"Montserrat\", sans-serif",
					"fontFace": [
						{
							"fontFamily": "Montserrat",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "100 900",
							"src": ["file:./assets/fonts/montserrat/Montserrat-VariableFont_wght.ttf"]
						}
					]
				},
				{
					"name": "Just Sans",
					"slug": "just-sans",
					"fontFamily": "\"Just Sans\", sans-serif",
					"fontFace": [
						{
							"fontFamily": "\"Just Sans\"",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": ["file:./assets/fonts/justsans/just-sans-regular.otf"]
						},
						{
							"fontFamily": "\"Just Sans\"",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "800",
							"src": ["file:./assets/fonts/justsans/just-sans-extrabold.otf"]
						}
					]
				}
			]
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header",
			"title": "Header"
		},
		{
			"area": "footer",
			"name": "footer",
			"title": "Footer"
		}
	]
}